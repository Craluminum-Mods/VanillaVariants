            "name": "VanillaVariants.BlockName",
            "properties": {
                "parts": ["block-trough-genericwood-small-*", " (", "material-{material}", ")"]
            }
        },
        {
            "name": "VanillaVariants.BlockDescription",
            "properties": {
                "parts": ["blockdesc-trough-genericwood-small-*"]
            }
        }, { "name": "NWOrientable" }
