    },
    "textures": {
        "charred": { "base": "game:block/wood/debarked/{material}" },
        "aged": { "base": "game:block/wood/debarked/{material}" },
        "wood": { "base": "game:block/wood/debarked/{material}" }
         // billion other textures are added by code
    },
    "sounds": {
        "place": "game:block/planks",
