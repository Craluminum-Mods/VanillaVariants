        {
            "name": "VanillaVariants.BlockName",
            "properties": {
                "parts": ["block-slantedroofingtop-sod-*", " (", "material-{material}", ")"]
            }
        },
        { "name": "HorizontalOrientable", "properties": { "dropBlockFace": "east", "drop": { "type": "block", "code": "slantedroofingtop-sod-{material}-east-free" } } }
    ],
    "variantgroups": [
        //
        { "code": "type", "states": ["sod"] },
        { "code": "material", "loadFromProperties": "block/wood" },
        { "loadFromProperties": "abstract/horizontalorientation" },
        { "code": "cover", "states": ["free", "snow"] }
    ],
    "attributes": {
        "handbook": { "groupBy": ["slantedroofingtop-sod-*"] }
    },
    "shapeByType": {
        "*-north-*": { "base": "game:block/roofing/main-top/sod-{cover}", "rotateY": 90 },
        "*-west-*": { "base": "game:block/roofing/main-top/sod-{cover}", "rotateY": 180 },
        "*-south-*": { "base": "game:block/roofing/main-top/sod-{cover}", "rotateY": 270 },
